<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="16dp"
			android:paddingTop="8dp"
			android:paddingRight="16dp"
			android:paddingBottom="8dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/back"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:src="@drawable/ic_arrow_back_black"
				android:scaleType="center" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			android:layout_weight="1">
			<ScrollView
				android:id="@+id/vscroll1"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="7dp"
					android:background="#F5F5F5"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="4dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:paddingBottom="8dp"
						android:text="User Details"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#176B87" />
					<TextView
						android:id="@+id/textview3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="First Name"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<EditText
							android:id="@+id/first_name"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingTop="8dp"
							android:paddingRight="16dp"
							android:paddingBottom="8dp"
							android:background="@android:color/transparent"
							android:textSize="16sp"
							android:textColor="#000000"
							android:textColorHint="#607D8B" />
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Middle Name (optional)"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview3"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<EditText
							android:id="@+id/middle_name"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingTop="8dp"
							android:paddingRight="16dp"
							android:paddingBottom="8dp"
							android:background="@android:color/transparent"
							android:textSize="16sp"
							android:textColor="#000000"
							android:textColorHint="#607D8B" />
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Last Name"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview4"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<EditText
							android:id="@+id/last_name"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingTop="8dp"
							android:paddingRight="16dp"
							android:paddingBottom="8dp"
							android:background="@android:color/transparent"
							android:textSize="16sp"
							android:textColor="#000000"
							android:textColorHint="#607D8B" />
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Sex"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview5"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear6"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingRight="16dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/sex"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:textSize="16sp"
								android:textColor="#000000"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/sex_option_icon"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:src="@drawable/ic_arrow_drop_down_black"
								android:scaleType="center" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview7"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Date of Birth"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview6"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear7"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingRight="16dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/birthday"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:textSize="16sp"
								android:textColor="#000000"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/birthday_icon"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="5dp"
								android:src="@drawable/calendar"
								android:scaleType="centerInside" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview8"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Email"
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview7"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear8"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingRight="16dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/email"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:textSize="16sp"
								android:textColor="#000000"
								android:layout_weight="1" />
							<ImageView
								android:id="@+id/email_icon"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="5dp"
								android:src="@drawable/ic_email_black"
								android:scaleType="centerInside" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="8dp"
						android:paddingTop="8dp"
						android:paddingRight="8dp"
						android:text="Contact No."
						android:textSize="10sp"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview8"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_margin="1dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear9"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingRight="16dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/contact_icon"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:src="@drawable/down"
								android:scaleType="center" />
							<TextView
								android:id="@+id/country_code"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:text="+63"
								android:textSize="16sp"
								android:textColor="#000000" />
							<EditText
								android:id="@+id/contact"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:padding="8dp"
								android:background="@android:color/transparent"
								android:textSize="16sp"
								android:textColor="#000000"
								android:textColorHint="#607D8B"
								android:inputType="phone"
								android:maxLength="10" />
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Booking Details"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview9"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="1dp"
						android:layout_marginTop="1dp"
						android:layout_marginRight="1dp"
						android:layout_marginBottom="7dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear10"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<androidx.cardview.widget.CardView
								android:id="@+id/cardview10"
								android:layout_width="70dp"
								android:layout_height="70dp"
								android:orientation="vertical"
								app:cardElevation="0dp"
								app:cardCornerRadius="5dp">
								<ImageView
									android:id="@+id/dorm_photo"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:src="@drawable/default_image"
									android:scaleType="centerCrop" />
							</androidx.cardview.widget.CardView>
							<LinearLayout
								android:id="@+id/linear11"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:orientation="vertical"
								android:layout_weight="1">
								<TextView
									android:id="@+id/dorm_title"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingTop="8dp"
									android:paddingRight="8dp"
									android:text="Dorm title"
									android:textSize="16sp"
									android:textStyle="bold"
									android:textColor="#176B87" />
								<TextView
									android:id="@+id/dorm_location"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingRight="8dp"
									android:paddingBottom="8dp"
									android:text="Location"
									android:textSize="12sp"
									android:textColor="#607D8B" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear12"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:orientation="vertical">
								<TextView
									android:id="@+id/dorm_monthly_price"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingTop="8dp"
									android:paddingRight="8dp"
									android:text="₱0,000"
									android:textSize="16sp"
									android:textStyle="bold"
									android:textColor="#176B87" />
								<TextView
									android:id="@+id/textview19"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingRight="8dp"
									android:paddingBottom="8dp"
									android:text="/month"
									android:textSize="10sp"
									android:textColor="#607D8B"
									android:layout_gravity="right" />
							</LinearLayout>
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview11"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="1dp"
						android:layout_marginTop="7dp"
						android:layout_marginRight="1dp"
						android:layout_marginBottom="7dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear16"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:padding="8dp"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/linear17"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview20"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Date"
									android:textSize="12sp"
									android:textColor="#607D8B"
									android:layout_weight="1" />
								<TextView
									android:id="@+id/date"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="dd/MMM/yyyy"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#607D8B" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear18"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview21"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Time"
									android:textSize="12sp"
									android:textColor="#607D8B"
									android:layout_weight="1" />
								<TextView
									android:id="@+id/time"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="00:00"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#607D8B" />
							</LinearLayout>
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview22"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Bedroom"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#607D8B" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview12"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="1dp"
						android:layout_marginTop="7dp"
						android:layout_marginRight="1dp"
						android:layout_marginBottom="7dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingTop="16dp"
							android:paddingRight="8dp"
							android:paddingBottom="16dp"
							android:orientation="vertical">
							<TextView
								android:id="@+id/bedroom_gender"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:text="Girl(s)"
								android:textSize="10sp"
								android:textStyle="bold"
								android:textColor="#000000" />
							<androidx.recyclerview.widget.RecyclerView
								android:id="@+id/recyclerview1"
								android:layout_width="match_parent"
								android:layout_height="wrap_content" />
							<LinearLayout
								android:id="@+id/linear36"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingTop="8dp"
								android:paddingRight="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview43"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="Total"
									android:textSize="12sp"
									android:textColor="#607D8B"
									android:layout_weight="1" />
								<TextView
									android:id="@+id/amount_total_bed"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="₱0,000"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#607D8B" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear24"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview26"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="One Month Advance"
									android:textSize="12sp"
									android:textColor="#607D8B"
									android:layout_weight="1" />
								<TextView
									android:id="@+id/amount_advance"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="₱0,000"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#607D8B" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/linear25"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:paddingLeft="8dp"
								android:paddingRight="8dp"
								android:paddingBottom="8dp"
								android:orientation="horizontal">
								<TextView
									android:id="@+id/textview27"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="One Month Deposit"
									android:textSize="12sp"
									android:textColor="#607D8B"
									android:layout_weight="1" />
								<TextView
									android:id="@+id/amount_deposit"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:text="₱0,000"
									android:textSize="12sp"
									android:textStyle="bold"
									android:textColor="#607D8B" />
							</LinearLayout>
						</LinearLayout>
					</androidx.cardview.widget.CardView>
					<TextView
						android:id="@+id/textview11"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Payment Details"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#176B87" />
					<androidx.cardview.widget.CardView
						android:id="@+id/cardview13"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="1dp"
						android:layout_marginTop="1dp"
						android:layout_marginRight="1dp"
						android:layout_marginBottom="7dp"
						android:orientation="vertical"
						app:cardElevation="2dp"
						app:cardCornerRadius="5dp">
						<LinearLayout
							android:id="@+id/linear26"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:padding="8dp"
